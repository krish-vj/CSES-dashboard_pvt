{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "CSES EXTENSION",
    "description": "Enhance your CSES experience with notes, tags, stats, and automation tools.",
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
    {
      "matches": ["https://cses.fi/*"],
      "js": ["src/content-script/find.tsx"]
    }
  ],
    "permissions": [
        "scripting", "activeTab", "storage"
    ],
    "host_permissions":[
        "https://cses.fi/*","http://cses.fi/*"
    ],
    "icons":{
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }



}